# Rename this file to .env and fill in your API keys

# API key for a web search API (e.g., Bing Search API)
WEB_SEARCH_API_KEY=your_web_search_api_key_here

# API endpoint for the web search API (example for Bing)
WEB_SEARCH_API_ENDPOINT=https://api.bing.microsoft.com/v7.0/search

# API key for OpenAI or other generative AI service
OPENAI_API_KEY=your_openai_api_key_here
